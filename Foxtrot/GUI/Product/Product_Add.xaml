<Page x:Class="Foxtrot.GUI.Product.Product_Add"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Foxtrot.GUI.Product"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1366"
      Background="White"
      Title="Product_Add">

    <Grid>
        <!-- Liste over produkt start -->
        <DataGrid x:Name="dataGrid_Product_List" HorizontalAlignment="Left" Margin="330,17,-1.6,0" VerticalAlignment="Top" Height="367" Background="White" Width="1038"/>
        <Label x:Name="label_Product_List" Content="Liste over produkter" HorizontalAlignment="Left" Margin="764,-5,0,0" VerticalAlignment="Top" Height="31" Width="121"/>
        <!-- Liste over produkt slut -->
       

        <!-- Søg Produkt Start -->
        <Border x:Name="border_Name" BorderBrush="Black" BorderThickness="3" HorizontalAlignment="Left" Height="377" VerticalAlignment="Top" Width="306" Margin="26,17,0,0"/>
        <Label x:Name="label_Product_Search" Content="&#x9;Søg" HorizontalAlignment="Left" Margin="95,31,0,0" VerticalAlignment="Top" Width="126"/>
        <Label x:Name="label1_Product_SearchName" Content="Søg Navn:" HorizontalAlignment="Left" Margin="55,71,0,0" VerticalAlignment="Top" Width="66"/>
        <TextBox x:Name="textBox_Product_SearchName" HorizontalAlignment="Left" Height="36" Margin="165,68,0,0" TextWrapping="Wrap" Text="{Binding Name}" VerticalAlignment="Top" Width="127" Tag="Navn*"/>
        <Label x:Name="label_Product_SearchID" Content="Søg ID" HorizontalAlignment="Left" Margin="64,127,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_SearchID" HorizontalAlignment="Left" Height="23" Margin="163,130,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_SearchArrangement" Content="Søg Arrangement" HorizontalAlignment="Left" Margin="44,176,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_SearchArrangement" HorizontalAlignment="Left" Height="23" Margin="158,181,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_SearchCategory" Content="Søg Kategori" HorizontalAlignment="Left" Margin="56,226,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_SearchCategory" HorizontalAlignment="Left" Height="23" Margin="161,237,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_SearchDate" Content="Søg Dato" HorizontalAlignment="Left" Margin="56,286,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.293,5.453"/>
        <TextBox x:Name="textBox_Product_SearchDate" HorizontalAlignment="Left" Height="23" Margin="149,294,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <!-- Søg Produkt Slut -->

        
        <!-- Opret Produkt Start -->
        <Label x:Name="label_Product_AddNew" Content="Opret nyt produkt" HorizontalAlignment="Left" Margin="643,408,0,0" VerticalAlignment="Top" Width="113"/>
        <Label x:Name="label_Product_Add_Name" Content="Navn" HorizontalAlignment="Left" Margin="68,426,0,0" VerticalAlignment="Top" Width="49" RenderTransformOrigin="1.314,1.234"/>
        <TextBox x:Name="textBox_Product_Add_Name" HorizontalAlignment="Left" Height="23" Margin="30,473,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Adress" Content="Adresse" HorizontalAlignment="Left" Margin="80,551,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Adress" HorizontalAlignment="Left" Height="23" Margin="43,584,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Longtitude" Content="Longtitude" HorizontalAlignment="Left" Margin="237,426,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Longtitude" HorizontalAlignment="Left" Height="23" Margin="218,486,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Latitude" Content="Latitude" HorizontalAlignment="Left" Margin="276,554,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Latitude" HorizontalAlignment="Left" Height="23" Margin="242,581,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Product_Add_ContactPhone" HorizontalAlignment="Left" Height="23" Margin="394,486,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_ContactPhone" Content="Telefon" HorizontalAlignment="Left" Margin="422,428,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Product_Add_ContactEmail" Content="Email" HorizontalAlignment="Left" Margin="436,553,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_ContactEmail" HorizontalAlignment="Left" Height="23" Margin="383,573,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_ContactFax" Content="Fax" HorizontalAlignment="Left" Margin="615,448,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,0.109"/>
        <TextBox x:Name="textBox_Product_Add_ContactFax" HorizontalAlignment="Left" Height="23" Margin="571,491,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_CreationDate" Content="Oprettelses dato" HorizontalAlignment="Left" Margin="598,547,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_CreationDate" HorizontalAlignment="Left" Height="23" Margin="581,582,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Price" Content="Pris" HorizontalAlignment="Left" Margin="790,444,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Príce" HorizontalAlignment="Left" Height="23" Margin="762,488,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Description" Content="Beskrivelse" HorizontalAlignment="Left" Margin="799,561,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Description" HorizontalAlignment="Left" Height="23" Margin="781,595,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_ExtraDescription" Content="Ekstra Beskrivelse" HorizontalAlignment="Left" Margin="975,455,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_ExtraDescription" HorizontalAlignment="Left" Height="23" Margin="934,490,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Availability" Content="Tilgængelighed" HorizontalAlignment="Left" Margin="967,557,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_Availability" HorizontalAlignment="Left" Height="23" Margin="964,597,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Product_Add_Website" Content="Hjemmeside" HorizontalAlignment="Left" Margin="1150,452,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Product_Add_CanonicalUrl" Content="URL" HorizontalAlignment="Left" Margin="1162,545,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Product_Add_CanonicalUrl" HorizontalAlignment="Left" Height="23" Margin="1126,598,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Product_Add_Website" HorizontalAlignment="Left" Height="23" Margin="1125,490,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <!-- Opret Produkt Slut -->

    </Grid>
</Page>
