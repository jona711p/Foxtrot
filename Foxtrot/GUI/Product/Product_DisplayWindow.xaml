<Window x:Class="Foxtrot.GUI.Product.Product_DisplayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Foxtrot.GUI.Product"
        mc:Ignorable="d"
        Title="Product_DisplayWindow" Height="600" Width="600">
    <Grid>
        <TextBox x:Name="textBox_Product_DisplayWindow_Name" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,85,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Adress" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,131,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Longitude" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,154,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Latitude" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,177,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Phone" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,200,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Email" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,223,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Fax" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,246,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Price" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,269,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Description" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,292,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_ExtraDescription" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,315,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_CanonicalURL" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,338,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Website" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="30,361,0,0"/>

    </Grid>
</Window>
