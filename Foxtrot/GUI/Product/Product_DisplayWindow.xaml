<Window x:Class="Foxtrot.GUI.Product.Product_DisplayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Foxtrot.GUI.Product"
        mc:Ignorable="d"
        Title="Product_DisplayWindow" Height="600" Width="600">
    <Grid>
        <TextBox x:Name="textBox_Product_DisplayWindow_Name" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,14,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Adress" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,37,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Longitude" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,60,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Latitude" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,83,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Phone" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,106,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Email" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,129,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Fax" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,152,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Price" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,175,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Description" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,198,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_ExtraDescription" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,221,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_CanonicalURL" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,244,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Website" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,267,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_City" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,310,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_MainCategory" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,333,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Category" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,356,0,0"/>
        <TextBox x:Name="textBox_Product_DisplayWindow_Availablity" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="28,379,0,0"/>

        <Image x:Name="image_Product_DisplayWindow_1" HorizontalAlignment="Left" Height="110" Margin="472,0,0,0" VerticalAlignment="Top" Width="110">
            <Image.ToolTip>
                <ToolTip DataContext="{Binding PlacementTarget, 
            RelativeSource={RelativeSource Self}}">
                    <Border BorderBrush="Black" BorderThickness="1" Margin="5,7,5,5">
                        <Image Source="{Binding Source}" Stretch="None" />
                    </Border>
                </ToolTip>
            </Image.ToolTip>
        </Image>
        <Image x:Name="image_Product_DisplayWindow_2" HorizontalAlignment="Left" Height="110" Margin="472,115,0,0" VerticalAlignment="Top" Width="110">
            <Image.ToolTip>
                <ToolTip DataContext="{Binding PlacementTarget, 
            RelativeSource={RelativeSource Self}}">
                    <Border BorderBrush="Black" BorderThickness="1" Margin="5,7,5,5">
                        <Image Source="{Binding Source}" Stretch="None" />
                    </Border>
                </ToolTip>
            </Image.ToolTip>
        </Image>
        <Image x:Name="image_Product_DisplayWindow_3" HorizontalAlignment="Left" Height="110" Margin="472,244,0,0" VerticalAlignment="Top" Width="110">
            <Image.ToolTip>
                <ToolTip DataContext="{Binding PlacementTarget, 
            RelativeSource={RelativeSource Self}}">
                    <Border BorderBrush="Black" BorderThickness="1" Margin="5,7,5,5">
                        <Image Source="{Binding Source}" Stretch="None" />
                    </Border>
                </ToolTip>
            </Image.ToolTip>
        </Image>
        <Image x:Name="image_Product_DisplayWindow_4" HorizontalAlignment="Left" Height="110" Margin="472,379,0,0" VerticalAlignment="Top" Width="110">
            <Image.ToolTip>
                <ToolTip DataContext="{Binding PlacementTarget, 
            RelativeSource={RelativeSource Self}}">
                    <Border BorderBrush="Black" BorderThickness="1" Margin="5,7,5,5">
                        <Image Source="{Binding Source}" Stretch="None" />
                    </Border>
                </ToolTip>
            </Image.ToolTip>
        </Image>




    </Grid>
</Window>
