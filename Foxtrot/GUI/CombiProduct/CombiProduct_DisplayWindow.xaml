<Page x:Class="Foxtrot.GUI.CombiProduct.CombiProduct_DisplayWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Foxtrot.GUI.CombiProduct"
      mc:Ignorable="d" 
      Background="White"
      Title="{Binding Name}" Height="800" Width="700">

    <Grid>
        <Label x:Name="label_CombiProduct_DisplayWindow_Name" Content="Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="16,60,0,0" Width="643" FontWeight="Bold" FontSize="20"/>
    </Grid>
</Page>
