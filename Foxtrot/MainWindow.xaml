<Window x:Class="Foxtrot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Foxtrot"
        mc:Ignorable="d"
        Title="Foxtrot" Height="768" Width="1366">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Menu">
                    <MenuItem Header="Forside"/>
                </MenuItem>
                <MenuItem Header="Bruger">
                    <MenuItem Header="Opret" />
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
                <MenuItem Header="Produkter">
                    <MenuItem Header="Opret"/>
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
                <MenuItem Header="Combiprodukter">
                    <MenuItem Header="Opret"/>
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
                <MenuItem Header="Events">
                    <MenuItem Header="Opret"/>
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
            </Menu>
            <StackPanel></StackPanel>
        </DockPanel>
    </Grid>
</Window>
