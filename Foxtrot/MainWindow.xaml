<Window x:Class="Foxtrot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Foxtrot"
        mc:Ignorable="d"
        Title="Foxtrot" Height="900" Width="1450">
    <Grid>

        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Menu">
                    <MenuItem Header="Forside" Click="MenuItem_Menu_Frontpage_OnClick"/>
                </MenuItem>
                <MenuItem Header="Bruger">
                    <MenuItem Header="Opret" Click="MenuItem_User_Create_OnClick"/>
                    <MenuItem Header="Rediger" Click="MenuItem_User_Edit_OnClick"/>
                </MenuItem>
                <MenuItem Header="Produkter">
                    <MenuItem Header="Opret" Click="MenuItem_Product_Create_OnClick"/>
                    <MenuItem Header="Rediger" Click="MenuItem_Product_Edit_OnClick"/>
                </MenuItem>
                <MenuItem Header="Combiprodukter">
                    <MenuItem Header="Opret"/>
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
                <MenuItem Header="Events">
                    <MenuItem Header="Opret"/>
                    <MenuItem Header="Rediger"/>
                    <MenuItem Header="Slet"/>
                </MenuItem>
            </Menu>
        </DockPanel>
        <TextBox x:Name="textBox" Text=""  Width="668" Margin="366,707,358,26" IsReadOnly="True" HorizontalContentAlignment="Center" HorizontalAlignment="Center" />
        <ComboBox x:Name="comboBox_Main_Usertype" Text="Vælg bruger" HorizontalAlignment="Left" Margin="1128,91,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Main_Usertype" Content="Vælg bruger" HorizontalAlignment="Left" Margin="1128,60,0,0" VerticalAlignment="Top" Width="100"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Center" Height="709" Margin="10,134,10,0" VerticalAlignment="Top" Width="1422">
            <Frame x:Name="MainFrame" Content="{Binding}" Width="1366"  Margin="9,9,3,337" HorizontalAlignment="Center"/>
        </Border>

    </Grid>
</Window>
